<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- 映射dao接口的全路径，id映射接口的方法名 -->
<mapper namespace="com.cssl.dao.UserDao">
    <insert id="zhuche" parameterType="yhb">
         INSERT INTO `yhb`(`Yh_mc`,`Yh_xbid`,`Yh_admin`,`Yh_pwd`,`Yh_sjhm`,`Yh_lxid`)
         VALUES(#{Yh_mc},#{Yh_xbid},#{Yh_admin},#{Yh_pwd},#{Yh_sjhm},#{Yh_lxid})
    </insert>

    <select id="denglu" resultType="yhb">
          SELECT * FROM `yhb` WHERE `Yh_admin`=#{Yh_admin} AND `Yh_pwd`=#{Yh_pwd}
    </select>

    <select id="zhucheyz" parameterType="String" resultType="int">
          SELECT COUNT(*) FROM `yhb` WHERE `Yh_admin`=#{Yh_admin}
    </select>
</mapper>